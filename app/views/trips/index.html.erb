<p id="notice"><%= notice %></p>

<h1>Listing Trips</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @trips.each do |trip| %>
      <tr>
        <td>
          <%= trip.trip_id %>
        </td>
        <td>
          <%= trip.start_date %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
